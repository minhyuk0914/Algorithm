      "source": [
        "n = int(input())\n",
        "\n",
        "stack = []\n",
        "\n",
        "for i in range(n):\n",
        "    num = int(input())\n",
        "    if num != 0:\n",
        "        stack.append(num)\n",
        "    else:\n",
        "        stack.pop()\n",
        "\n",
        "ans = 0 # 스택 데이터 총합\n",
        "for x in stack:\n",
        "    ans += x\n",
        "\n",
        "print(ans)"
      ],
